{
  "name": "@khs-crm/frontend",
  "version": "1.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:check": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives",
    "lint:strict": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "verify": "node verify-components.cjs",
    "verify:full": "node test-runner.cjs",
    "verify:quick": "node quick-test.cjs",
    "precommit": "node verify-components.cjs",
    "build:github": "vite build --config vite.config.github.ts",
    "predeploy": "npm run build:github",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@prisma/client": "^6.14.0",
    "@tanstack/react-query": "^5.17.9",
    "@types/react-window": "^1.8.8",
    "@vercel/node": "^5.3.13",
    "axios": "^1.6.5",
    "date-fns": "^3.2.0",
    "dotenv": "^17.2.1",
    "idb": "^8.0.0",
    "prisma": "^5.22.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.62.0",
    "react-router-dom": "^6.21.2",
    "react-toastify": "^11.0.5",
    "react-window": "^1.8.11",
    "workbox-window": "^7.0.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@tanstack/react-query-devtools": "^5.84.2",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.2.0",
    "@vitest/ui": "^1.2.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "gh-pages": "^6.3.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.33",
    "prettier": "^3.2.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vite-plugin-pwa": "^0.17.4",
    "vitest": "^1.2.0"
  }
}
